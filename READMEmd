
api_server = "http://localhost:8080"
image_server = "http://localhost:8080"
// signup
r = await fetch(api_server+"/user/signup",
    {
        method: "POST", body: JSON.stringify(
            {
                "name": "asdf",
                "userid": "qwer",
            }
        )
        , headers: { "Content-Type": "application/json" }
    })

// login
async function getToken(id) {
    r = await fetch(api_server+"/user/login?id=" + id,
        {
            method: "GET",
        })
    res = await r.json()
    return res["token"]
}
token = await getToken("qwer")

// upload content
r = await fetch(api_server+"/contents/upload",
    {
        method: "POST", body: JSON.stringify(
            {
                "content": "asdf",
                "images": 3,
            }
        ),
        headers: {
            "Authorization": token,
            "Content-Type": "application/json",
        }
    })


// upload images
r = await fetch(image_server+"",
{
    method: "POST", body: JSON.stringify(
        {
            "content": "asdf",
            "images": 3,
        }
    ),
    headers: {
        "Authorization": token,
        "Content-Type": "application/json",
    }
})